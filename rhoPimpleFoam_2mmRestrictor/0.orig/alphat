FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            alphatWallFunction;
        Prt             0.85;
        value           uniform 0;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet_ur
    {
        type            zeroGradient;
    }
    outlet_wr
    {
        type            zeroGradient;
    }

}

